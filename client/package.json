{
  "name": "nautilus",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "main": "dist/main/main.js",
  "homepage": "https://github.com/ATOMGAMERAGA/nautilus",
  "author": {
    "name": "Nautilus Team",
    "email": "nautilus@atomland.xyz"
  },
  "description": "Nautilus - Modern Chat Application",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build:all": "npm run build && npm run build:electron",
    "dist:linux": "npm run build:all && electron-builder --linux deb AppImage --x64",
    "dist:win": "npm run build:all && electron-builder --win nsis --x64",
    "dist:all": "npm run build:all && electron-builder --linux deb AppImage --win nsis --x64",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "vite & sleep 3 && electron .",
    "build:android": "vite build --mode android",
    "android:dev": "npm run build:android && npx cap sync android && npx cap open android",
    "android:run": "npx cap run android",
    "android:build-debug": "npm run build:android && npx cap sync android && cd android && ./gradlew assembleDebug",
    "android:build-release": "npm run build:android && npx cap sync android && cd android && ./gradlew assembleRelease bundleRelease",
    "cap:sync": "npx cap sync"
  },
  "dependencies": {
    "@capacitor/android": "^8.1.0",
    "@capacitor/app": "^8.0.1",
    "@capacitor/camera": "^8.0.1",
    "@capacitor/cli": "^8.1.0",
    "@capacitor/core": "^8.1.0",
    "@capacitor/filesystem": "^8.1.1",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/network": "^8.0.1",
    "@capacitor/preferences": "^8.0.1",
    "@capacitor/push-notifications": "^8.0.1",
    "@capacitor/splash-screen": "^8.0.1",
    "@capacitor/status-bar": "^8.0.1",
    "@hookform/resolvers": "^5.2.2",
    "axios": "^1.13.5",
    "electron-store": "^11.0.2",
    "emoji-picker-react": "^4.18.0",
    "lucide-react": "^0.563.0",
    "mediasoup-client": "^3.18.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.71.1",
    "react-router-dom": "^7.13.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^40.4.0",
    "electron-builder": "^26.7.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1"
  }
}
